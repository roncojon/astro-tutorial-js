{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts\\/post-1\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts\\/post-2\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts\\/post-3\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts\\/post-4\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/tags\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/tags\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}